{
  "root": true,
  "env": {
    "node": true
  },
  "extends": ["plugin:vue/vue3-essential", "eslint:recommended", "plugin:prettier/recommended", "@vue/typescript"],
  "parserOptions": {
    "parser": "@typescript-eslint/parser"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "error",
    "prettier/prettier": "error"
  }
}
